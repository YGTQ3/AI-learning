# Supabase配置
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# 应用配置
VITE_APP_NAME=AI学习助手社交框架
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=基于React + Supabase的社交学习框架

# API配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# 功能开关
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_OFFLINE_MODE=true

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES_PER_UPLOAD=5
VITE_ALLOWED_FILE_TYPES=image/*,video/*,audio/*,application/pdf

# 社交功能配置
VITE_MAX_POST_LENGTH=1000
VITE_MAX_COMMENT_LENGTH=500
VITE_MAX_TAGS_PER_POST=10
VITE_DAILY_POST_LIMIT=50

# 开发环境配置
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

# 第三方服务
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_CRISP_WEBSITE_ID=

# PWA配置
VITE_PWA_NAME=AI学习助手
VITE_PWA_SHORT_NAME=学习助手
VITE_PWA_DESCRIPTION=智能社交学习平台
VITE_PWA_THEME_COLOR=#3b82f6
VITE_PWA_BACKGROUND_COLOR=#ffffff
